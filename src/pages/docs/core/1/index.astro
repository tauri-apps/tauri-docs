---
import { getCollection } from 'astro:content'
import Layout from '../../../../layouts/Layout.astro'

const collection = await getCollection('docs-core-1')
---

<Layout title="Core Docs">
  <h1>Core Docs</h1>
  <p>This is a custom astro page</p>
  <ul>
    {
      collection.map((entry) => (
        <li>
          <a href={`${Astro.url.pathname}/${entry.slug}`}>{entry.slug}</a>
        </li>
      ))
    }
  </ul>
</Layout>
